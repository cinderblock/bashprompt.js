{
    "name": "bashprompt",
    "description": "A bash prompt framework in node.js",
    "version": "0.1.0",
    "author": "Raül Pérez <repejota@gmail.com>",
    "contributors": "https://github.com/repejota/bashprompt.js/graphs/contributors",
    "preferGlobal": true,
    "main": "./lib/index.js",
    "directories": {
        "lib": "lib",
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/repejota/bashprompt.js.git"
    },
    "bugs": {
        "url": "https://github.com/repejota/bashprompt.js/issues"
    },
    "homepage": "https://github.com/repejota/bashprompt.js",
    "dependencies": {
        "nomnom": "*"
    },
    "analyze": false,
    "devDependencies": {
        "mocha": "*",
        "istanbul": "*",
        "matchdep": "*",
        "grunt": "*",
        "grunt-jsdoc": "*",
        "grunt-contrib-jshint": "*",
        "grunt-contrib-clean": "*",
        "grunt-lint-bash": "*",
        "grunt-shell": "*"
    },
    "bin": {
        "bp": "./bin/bp"
    },
    "scripts": {
        "prepublish": "npm prune",
        "test": "mocha test --colors --recursive --reporter spec",
        "test-watch": "mocha test --watch --colors --recursive --reporter dot",
        "coverage": "./node_modules/istanbul/lib/cli.js cover --print detail ./node_modules/mocha/bin/_mocha -- --colors --recursive -R spec"
    },
    "licenses": [
        {
            "type": "MIT",
            "url": "http://github.com/repejota/bashprompt.js/blob/master/LICENSE"
        }
    ],
    "engine": {
        "node": ">=0.10.0"
    },
    "keywords": [
        "ansi",
        "terminal",
        "colors",
        "bash",
        "propmt",
        "ps1",
        "ps2",
        "shell"
    ],
    "readmeFilename": "README.md",
    "_id": "bashprompt@0.1.0",
    "_from": "bashprompt@*"
}
